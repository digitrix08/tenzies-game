{
    "homepage": ".",
    "name": "tenzies",
    "version": "0.1.0",
    "private": true,
    "main": "public/electron.js",
    "description": "Tenzies game",
    "author": {
        "name": "Arup Patel",
        "email": "mastersoft6@gmail.com"
    },
    "build": {
        "appId": "app.tenzies.game",
        "win": {
            "target": "portable",
            "icon": "src/assets/images/red-die.png"
        }
    },
    "dependencies": {
        "@testing-library/jest-dom": "^5.15.1",
        "@testing-library/react": "^11.2.7",
        "@testing-library/user-event": "^12.8.3",
        "react": "^17.0.2",
        "react-confetti": "^6.0.1",
        "react-dom": "^17.0.2",
        "react-scripts": "^4.0.3",
        "react-use": "^17.3.1",
        "web-vitals": "^1.1.2"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "electron-dev": "concurrently -k \"yarn start\" \"yarn electron\"",
        "electron": "wait-on tcp:3000 && electron .",
        "electron-checks": "electronegativity -i .",
        "electron-prod": "react-scripts build && electron .",
        "create-package": "electron-builder"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@doyensec/electronegativity": "^1.10.2",
        "concurrently": "^7.6.0",
        "electron": "^22.1.0",
        "electron-builder": "^23.6.0",
        "electron-reload": "^2.0.0-alpha.1",
        "gh-pages": "^3.2.3",
        "wait-on": "^7.0.1"
    }
}
